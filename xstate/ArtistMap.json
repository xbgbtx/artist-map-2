{
  "id": "ArtistMap",
  "initial": "Init",
  "states": {
    "Init": {
      "on": {
        "PageLoaded": {
          "target": "FetchingWikiData"
        }
      }
    },
    "FetchingWikiData": {
      "on": {
        "WikiDataFetchComplete": {
          "target": "BuildingMap"
        }
      }
    },
    "BuildingMap": {
      "on": {
        "MapBuildComplete": {
          "target": "MapDisplay"
        }
      }
    },
    "MapDisplay": {}
  }
}
