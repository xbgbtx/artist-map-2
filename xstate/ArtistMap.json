{
  "id": "ArtistMap",
  "initial": "Start",
  "states": {
    "Start": {
      "on": {
        "PageLoaded": {
          "target": "Fetch WikiData Marker Data"
        }
      }
    },
    "Fetch WikiData Marker Data": {
      "on": {
        "WikiData Fetch Complete": {
          "target": "Build Map"
        }
      }
    },
    "Build Map": {
      "on": {
        "Map Build Complete": {
          "target": "Map Display"
        }
      }
    },
    "Map Display": {}
  }
}
